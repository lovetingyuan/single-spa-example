{
  "navbar": {
    "port": 8081,
    "mountPath": "/",
    "publicPath": "/navbar/",
    "serve": "npx cross-env PORT=${port} npm run serve"
  },
  "vue-app": {
    "port": 8082,
    "mountPath": "/vue-singleapp",
    "serve": "npm run serve -- --port ${port} --public=${entry}",
    "default": true
  },
  "react-app": {
    "port": 8083,
    "mountPath": "/react-singleapp",
    "output": "build",
    "serve": "npx cross-env BROWSER=none PORT=${port} WDS_SOCKET_PORT=${port} npm start"
  },
  "_ng-app": {
    "serve": "ng serve --disable-host-check --port 8082 --deploy-url http://localhost:8082/ --live-reload false",
    "port": 8084,
    "mountPath": ""
  }
}
